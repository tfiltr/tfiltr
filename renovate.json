{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchDepTypes": [
        "dev-dependencies",
        "dependencies"
      ],
      "groupName": "dependencies",
      "rangeStrategy": "bump"
    }
  ]
}
